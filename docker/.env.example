# Example Docker Compose environment variables
# Copy this file to .env and customize for your setup

# General Settings
TZ=America/New_York
PUID=1000
PGID=1000

# Domain Configuration
DOMAIN=homelab.local

# Network Settings
NETWORK_SUBNET=172.20.0.0/16

# Data Directories
DATA_ROOT=/mnt/storage
CONFIG_ROOT=/opt/homelab

# Service Ports (if not using reverse proxy)
# PORTAINER_PORT=9000
# GRAFANA_PORT=3000
# PROMETHEUS_PORT=9090

# Passwords and Secrets (CHANGE THESE!)
# DB_PASSWORD=changeme
# ADMIN_PASSWORD=changeme
